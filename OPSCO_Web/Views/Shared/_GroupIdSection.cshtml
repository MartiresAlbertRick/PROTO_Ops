@model OPSCO_Web.Models.OSC_TeamGroupIds

<tr id="@Model.TGIId">
    <td class="col-md-4">
        @Html.DropDownList("GroupType", (IEnumerable<SelectListItem>)ViewBag.GroupTypes, "--Select Grouptype--", htmlAttributes: new { @class = "form-control input-sm", id = "cmb_" + Model.TGIId })
    </td>
    <td class="col-md-6">
        @Html.EditorFor(model => model.GroupId, new { htmlAttributes = new { @class = "form-control input-sm" } })
    </td>
    <td>
        <a class="btn btn-danger btn-xs" title="Remove Group Id">
            <i class="fa fa-trash"></i>
        </a>
    </td>
</tr>
